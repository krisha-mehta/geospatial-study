{
    "$schema": "https://raw.githubusercontent.com/revisit-studies/study/main/src/parser/StudyConfigSchema.json",
    "studyMetadata": {
        "title": "Uncertainty Visualizations for Geospatial Decision-Making Study",
        "version": "pilot",
        "authors": [
            "Krisha Mehta",
            "Julia Koschinsky",
            "Alex Kale"
        ],
        "date": "2025-08-29",
        "description": "This study investigates how different uncertainty representations influence decision-making",
        "organizations": [
            "University of Chicago"
        ]
    },
    "uiConfig": {
        "contactEmail": "krisha@uchicago.edu",
        "helpTextPath": "geospatial-dm/assets/help.md",
        "logoPath": "revisitAssets/dcl_logo.png",
        "withProgressBar": true,
        "autoDownloadStudy": false,
        "sidebar": false,
        "urlParticipantIdParam": "PROLIFIC_PID",
        "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**"
    },
    "baseComponents": {
        "pixel-map-d3": {
            "type": "website",
            "response": [
              {
                "id": "selectedCounty",
                "prompt": "Your selected answer:",
                "required": true,
                "location": "belowStimulus",
                "type": "reactive"
              }
            ],
            "path": "geospatial-dm/assets/d3-pixel.html",
            "instructionLocation": "aboveStimulus"
          },"hom-map-d3": {
            "type": "website",
            "response": [
              {
                "id": "selectedCounty",
                "prompt": "Your selected answer:",
                "required": true,
                "location": "belowStimulus",
                "type": "reactive"
              }
            ],
            "path": "geospatial-dm/assets/d3-hop.html",
            "instructionLocation": "aboveStimulus"
          }
    },
    "components": {
        "introduction": {
            "type": "markdown",
            "path": "geospatial-dm/assets/introduction.md",
            "response": []
        },
        "d3-pixel-1":{
            "baseComponent": "pixel-map-d3",
            "type":"website",
            "path": "geospatial-dm/assets/d3-pixel.html",
           "instruction": "Click a county to select it.",
            "parameters": {
                "stateFips": "35",
                "csvFile": "NMData.csv"
            }
        },
        "d3-pixel-2":{
            "baseComponent": "pixel-map-d3",
            "type":"website",
            "path": "geospatial-dm/assets/d3-pixel.html",
           "instruction": "Click a county to select it.",
            "parameters": {
                "stateFips": "04",
                "csvFile": "AZData.csv"
            }
        },
        "d3-hom-1":{
            "baseComponent": "hom-map-d3",
            "type":"website",
            "path": "geospatial-dm/assets/d3-hop.html",
           "instruction": "Click a county to select it.",
            "parameters": {
                "stateFips": "35",
                "csvFile": "NMData.csv"
            }
        },
        "d3-hom-2":{
            "baseComponent": "hom-map-d3",
            "type":"website",
            "path": "geospatial-dm/assets/d3-hop.html",
           "instruction": "Click a county to select it.",
            "parameters": {
                "stateFips": "04",
                "csvFile": "AZData.csv"
            }
        },
        "consent":{
            "type": "markdown",
            "path": "geospatial-dm/assets/consent.md",
            "nextButtonText": "Agree",
            "response": [
                {
                    "id": "prolificId",
                    "prompt": "Please enter your Prolific ID",
                    "required": true,
                    "location": "belowStimulus",
                    "type": "shortText",
                    "placeholder": "Prolific ID",
                    "paramCapture": "PROLIFIC_PID"
                },
                {
                "id": "accept",
                "prompt": "Do you consent to the study and wish to continue?",
                "required": true,
                "requiredValue": "yes",
                "location": "belowStimulus",
                "type": "radio",
                "options": [
                    {
                        "label": "Decline",
                        "value": "no"
                    },
                    {
                        "label": "Accept",
                        "value": "yes"
                    }
                ]
            }

            ]
        }
    },
    "sequence": {
        "order": "fixed",
        "components": [
            "introduction",
            "consent",
            "d3-pixel-1",
            "d3-pixel-2",
            "d3-hom-1",
            "d3-hom-2",
            {
                "order": "latinSquare",
                "numSamples": 1,
                "components": [
                    {
                        "order": "latinSquare",
                        "components": [
                            "pixel-map-1",
                            "pixel-map-2",
                            "pixel-map-3"
                        ]
                    },
                    {
                        "order": "latinSquare",
                        "components": [
                            "hop-map-1",
                            "hop-map-2",
                            "hop-map-3"
                        ]
                    }
                ]
            }
           
            
        ]

    }
}